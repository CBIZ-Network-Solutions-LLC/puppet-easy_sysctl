{
  "name": "qtechnologies-easy_sysctl",
  "version": "1.0.2",
  "author": "Q-Technologies",
  "summary": "Abstraction layer above augeasproviders_sysctl module to make using hiera easier and less verbose.",
  "license": "Apache-2.0",
  "source": "https://github.com/Q-Technologies/puppet-easy_sysctl.git",
  "project_page": "https://github.com/Q-Technologies/puppet-easy_sysctl",
  "issues_url": "https://github.com/Q-Technologies/puppet-easy_sysctl/issues",
  "dependencies": [
    {
      "name": "herculesteam-augeasproviders_sysctl",
      "version_requirement": ">= 2.2.0 < 3.0.0"
    },
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.14.0 < 6.0.0"
    }
  ],
  "data_provider": "hiera",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7.0",
        "8.0"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "SLES",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 7.0.0"
    }
  ],
  "tags": [
    "sysctl",
    "hiera"
  ],
  "pdk-version": "1.18.1",
  "template-url": "pdk-default#1.18.1",
  "template-ref": "tags/1.18.1-0-g3d2e75c"
}
